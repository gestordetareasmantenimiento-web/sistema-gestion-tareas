  t.id as id_de_la_tarea,
  t.estado as estado_de_la_tarea,
  t.id_inspector as id_inspector_en_la_tarea,
  insp.id as id_del_inspector,
  insp.nombre_completo as nombre_del_inspector,
  insp.id_supervisor as id_supervisor_del_inspector
FROM tareas t
LEFT JOIN usuarios insp ON t.id_inspector = insp.id
WHERE insp.id_supervisor = 14;</sql><current_tab id="0"/></tab_sql></sqlb_project>
